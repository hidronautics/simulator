<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <arg name="world" default="empty"/> 
  <arg name="paused" default="false"/>
  <arg name="use_sim_time" default="true"/>
  <arg name="gui" default="true"/>
  <arg name="headless" default="false"/>
  <arg name="debug" default="false"/>
  
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <!--arg name="world_name" value="$(find mybot_gazebo)/worlds/pool_world_2.world"/-->
    <!--<arg name="world_name" value="$(find mybot_gazebo)/worlds/turtlebot_playground.world"/>-->
    <arg name="world_name" value="$(find auv_gazebo)/worlds/singapore.world"/>
    <arg name="paused" value="$(arg paused)"/>
    <arg name="use_sim_time" value="$(arg use_sim_time)"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="headless" value="$(arg headless)"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>

<!--  <param name="pool" command="$(find xacro)/xacro '$(find pool)/urdf/pool.xacro'"/>-->

<!--  <node name="pool_spawn" pkg="gazebo_ros" type="spawn_model" output="screen"-->
<!--  args="-urdf -param pool -x 25 -y 12.5 -z 0 -R 0 -P 0 -Y 0 -model pool" />-->

  <include file="$(find spawn_sdf)/launch/spawn_sdf.launch">
    <arg name="robot_name" value="pool" />
    <arg name="x" value="25.0" />
    <arg name="y" value="12.5" />
    <arg name="z" value="-3.1" />
    <arg name="roll" value="0"/>
    <arg name="pitch" value="0"/>
    <arg name="yaw" value="0.0" />
    <arg name="sdf_robot_file" value="$(find spawn_sdf)/models/pool/model.sdf" />
  </include>

</launch>
